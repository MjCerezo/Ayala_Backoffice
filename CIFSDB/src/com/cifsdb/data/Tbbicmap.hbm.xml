        <property name="datereporteddt" type="timestamp">
            <column name="datereporteddt" length="23"/>
        </property>
        <property name="dateofbirth" type="timestamp">
            <column name="dateofbirth" length="23"/>
        </property>
        <property name="telephoneno" type="string">
            <column name="telephoneno" length="50"/>
        </property>
        <property name="datedisconnected" type="timestamp">
            <column name="datedisconnected" length="23"/>
        </property>
        <property name="pastdue" type="string">
