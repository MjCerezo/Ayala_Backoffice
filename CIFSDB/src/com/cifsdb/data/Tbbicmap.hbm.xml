        </property>
        <property name="birequestid" type="string">
            <column name="birequestid" length="20"/>
        </property>
        <property name="reportdate" type="timestamp">
            <column name="reportdate" length="23"/>
        </property>
        <property name="repo" type="string">
            <column name="repo" length="10"/>
        </property>
        <property name="reporttype" type="string">
            <column name="reporttype" length="10"/>
