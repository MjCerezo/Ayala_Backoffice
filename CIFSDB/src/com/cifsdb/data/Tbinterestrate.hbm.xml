<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbinterestrate" table="TBINTERESTRATE" catalog="ACACIADBNP" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="interestrate" type="big_decimal">
            <column name="interestrate" precision="18"/>
        </property>
        <property name="minamount" type="big_decimal">
            <column name="minamount" precision="18"/>
        </property>
        <property name="maxamount" type="big_decimal">
            <column name="maxamount" precision="18"/>
        </property>
    </class>
</hibernate-mapping>