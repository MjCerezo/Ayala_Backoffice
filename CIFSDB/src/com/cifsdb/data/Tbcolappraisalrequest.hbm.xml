        <property name="appno" type="string">
            <column name="appno" length="20"/>
        </property>
        <property name="referenceno" type="string">
            <column name="referenceno" length="50"/>
        </property>
        <property name="collateraltype" type="string">
            <column name="collateraltype" length="50"/>
        </property>
        <property name="typeappraisal" type="string">
            <column name="typeappraisal" length="50"/>
        </property>
        <property name="assignedappsupervisor" type="string">
            <column name="assignedappsupervisor" length="20"/>
        </property>
        <property name="requestedby" type="string">
            <column name="requestedby" length="50"/>
        </property>
        <property name="daterequested" type="timestamp">
            <column name="daterequested" length="23"/>
