        <property name="remarks" type="string">
            <column name="remarks" length="8000"/>
        </property>
        <property name="purposeofappraisal" type="string">
            <column name="purposeofappraisal"/>
        </property>
        <property name="collateralcategory" type="string">
            <column name="collateralcategory" length="2"/>
        </property>
        <property name="groupid" type="string">
            <column name="groupid" length="20"/>
        </property>
        <property name="newused" type="string">
            <column name="newused" length="2"/>
        </property>
        <property name="propertytype" type="string">
            <column name="propertytype" length="5"/>
        </property>
    </class>
</hibernate-mapping>