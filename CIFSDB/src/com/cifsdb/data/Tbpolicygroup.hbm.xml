<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbpolicygroup" table="TBPOLICYGROUP" catalog="ACACIADBNP" schema="dbo" dynamic-insert="false" dynamic-update="false">
