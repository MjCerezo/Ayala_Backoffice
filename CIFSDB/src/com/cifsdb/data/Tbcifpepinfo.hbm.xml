<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbcifpepinfo" table="TBCIFPEPINFO" catalog="AcaciaAyalaMembershipDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="cifno" type="string">
            <column name="cifno" length="50"/>
        </property>
        <property name="type" type="string">
            <column name="type" length="2"/>
        </property>
        <property name="govtype" type="string">
            <column name="govtype" length="50"/>
        </property>
        <property name="govname" type="string">
            <column name="govname" length="300"/>
        </property>
        <property name="position" type="string">
            <column name="position" length="300"/>
        </property>
    </class>
</hibernate-mapping>