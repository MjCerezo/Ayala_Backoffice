<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbappreltcpcondo" table="TBAPPRELTCPCONDO" catalog="ACACIADBNP" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="appraisalreportid" type="string">
            <column name="appraisalreportid" length="20"/>
        </property>
        <property name="appno" type="string">
            <column name="appno" length="20"/>
        </property>
        <property name="collateralid" type="integer">
            <column name="collateralid"/>
        </property>
        <property name="condounit" type="string">
            <column name="condounit" length="50"/>
        </property>
        <property name="tcp" type="big_decimal">
            <column name="tcp" precision="18"/>
        </property>
        <property name="grosstcp" type="big_decimal">
            <column name="grosstcp" precision="18"/>
        </property>
    </class>
</hibernate-mapping>