<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbhistory" table="TBHISTORY" catalog="AcaciaAyalaMembershipDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="cifno" type="string">
            <column name="cifno" length="50"/>
        </property>
        <property name="username" type="string">
            <column name="username" length="50"/>
        </property>
        <property name="historydatetime" type="timestamp">
            <column name="historydatetime" length="23"/>
        </property>
        <property name="eventdescription" type="string">
            <column name="eventdescription"/>
        </property>
        <property name="eventname" type="string">
            <column name="eventname"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="8000"/>
        </property>
        <property name="appno" type="binary">
            <column name="appno"/>
        </property>
        <property name="eventid" type="integer">
            <column name="eventid"/>
        </property>
        <property name="ipaddress" type="binary">
            <column name="ipaddress"/>
        </property>
    </class>
</hibernate-mapping>