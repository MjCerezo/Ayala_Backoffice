<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbsigcard" table="TBSIGCARD" catalog="ACACIADBNP" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="accountno" type="string">
            <column name="accountno" length="50"/>
        </property>
        <property name="sigbasecode" type="string">
            <column name="sigbasecode"/>
        </property>
        <property name="sigfilename" type="string">
            <column name="sigfilename" length="200"/>
        </property>
    </class>
</hibernate-mapping>