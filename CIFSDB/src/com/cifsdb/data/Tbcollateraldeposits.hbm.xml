<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cifsdb.data.Tbcollateraldeposits" table="TBCOLLATERALDEPOSITS" catalog="ACACIADBNP" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="collateralid" type="integer">
            <column name="collateralid"/>
            <generator class="identity"/>
        </id>
        <property name="referenceno" type="string">
            <column name="referenceno" length="50"/>
        </property>
        <property name="collateralstatus" type="string">
            <column name="collateralstatus" length="50"/>
        </property>
        <property name="appraisalstatus" type="string">
            <column name="appraisalstatus" length="50"/>
        </property>
        <property name="dateencoded" type="timestamp">
            <column name="dateencoded" length="23"/>
        </property>
        <property name="encodedby" type="string">
            <column name="encodedby" length="50"/>
        </property>
        <property name="registeredname" type="string">
            <column name="registeredname" length="100"/>
        </property>
        <property name="amount" type="big_decimal">
            <column name="amount" precision="20"/>
        </property>
        <property name="intrate" type="big_decimal">
            <column name="intrate" precision="20"/>
        </property>
        <property name="dateofplacement" type="timestamp">
            <column name="dateofplacement" length="23"/>
        </property>
        <property name="maturitydate" type="timestamp">
            <column name="maturitydate" length="23"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="300"/>
        </property>
        <property name="acctnumber" type="string">
            <column name="acctnumber" length="50"/>
        </property>
    </class>
</hibernate-mapping>