                <value>classpath:COOPDB.properties</value>
            </list>
        </property>
    </bean>

    <bean 
        id="COOPDBDataSource"
        class="org.springframework.jdbc.datasource.DriverManagerDataSource" 
        lazy-init="true">
        <property name="driverClassName" value="${COOPDB.driverClassName}"/>
