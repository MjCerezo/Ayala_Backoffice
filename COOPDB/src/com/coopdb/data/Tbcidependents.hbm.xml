<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbcidependents" table="TBCIDEPENDENTS" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="dependentid" type="integer">
            <column name="dependentid"/>
            <generator class="identity"/>
        </id>
        <property name="cireportid" type="string">
            <column name="cireportid" length="20"/>
        </property>
        <property name="fullname" type="string">
            <column name="fullname" length="300"/>
        </property>
        <property name="gender" type="string">
            <column name="gender" length="50"/>
        </property>
        <property name="dateofbirth" type="timestamp">
            <column name="dateofbirth" length="23"/>
        </property>
        <property name="age" type="integer">
            <column name="age"/>
        </property>
        <property name="fulladdress" type="string">
            <column name="fulladdress" length="300"/>
        </property>
        <property name="relationship" type="string">
            <column name="relationship" length="50"/>
        </property>
        <property name="otherrelationship" type="string">
            <column name="otherrelationship" length="100"/>
        </property>
    </class>
</hibernate-mapping>