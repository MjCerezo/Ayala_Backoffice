        <property name="seqmerch" type="integer">
            <column name="seqmerch"/>
        </property>
        <property name="seqno" type="integer">
            <column name="seqno"/>
        </property>
        <property name="seqoverride" type="integer">
            <column name="seqoverride"/>
        </property>
        <property name="seqtdc" type="integer">
            <column name="seqtdc"/>
