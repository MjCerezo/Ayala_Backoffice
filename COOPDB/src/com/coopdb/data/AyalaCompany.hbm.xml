<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.AyalaCompany" table="AyalaCompany" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="Id"/>
            <generator class="identity"/>
        </id>
        <property name="branch" type="string">
            <column name="Branch" length="120"/>
        </property>
        <property name="companyCode" type="string">
            <column name="CompanyCode" length="120"/>
        </property>
        <property name="companyName" type="string">
            <column name="CompanyName" length="120"/>
        </property>
        <property name="clusterHead" type="string">
            <column name="ClusterHead" length="120"/>
        </property>
        <property name="clusterHeadCode" type="string">
            <column name="ClusterHeadCode" length="120"/>
        </property>
        <property name="dateCreated" type="string">
            <column name="DateCreated" length="10"/>
        </property>
    </class>
</hibernate-mapping>