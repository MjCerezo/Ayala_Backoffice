<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbholiday" table="TBHOLIDAY" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="holDate" type="timestamp">
            <column name="hol_date" length="23"/>
        </property>
        <property name="holName" type="string">
            <column name="hol_name" length="50"/>
        </property>
        <property name="frequency" type="string">
            <column name="frequency" length="50"/>
        </property>
        <property name="area" type="string">
            <column name="area" length="50"/>
        </property>
        <property name="holidaytype" type="string">
            <column name="holidaytype" length="50"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="dateupdated" type="timestamp">
            <column name="dateupdated" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
    </class>
</hibernate-mapping>