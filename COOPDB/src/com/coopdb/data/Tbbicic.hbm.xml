<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbbicic" table="TBBICIC" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="bireportid" type="string">
            <column name="bireportid" length="20"/>
            <generator class="assigned"/>
        </id>
        <property name="cifno" type="string">
            <column name="cifno" length="20"/>
        </property>
        <property name="appno" type="string">
            <column name="appno" length="20"/>
        </property>
        <property name="dmsid" type="string">
            <column name="dmsid" length="50"/>
        </property>
        <property name="description" type="string">
            <column name="description"/>
        </property>
        <property name="birequestid" type="string">
            <column name="birequestid" length="20"/>
        </property>
        <property name="reportdate" type="timestamp">
            <column name="reportdate" length="23"/>
        </property>
    </class>
</hibernate-mapping>