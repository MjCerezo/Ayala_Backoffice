<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbgaaperbos" table="TBGAAPERBOS" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="boscode" type="string">
            <column name="boscode" length="50"/>
        </property>
        <property name="servicestatus" type="string">
            <column name="servicestatus" length="5"/>
        </property>
        <property name="amount" type="big_decimal">
            <column name="amount" precision="20"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
        <property name="dateupdated" type="timestamp">
            <column name="dateupdated" length="23"/>
        </property>
    </class>
</hibernate-mapping>