<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbcollateralsecurities" table="TBCOLLATERALSECURITIES" catalog="COOPDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="collateralid" type="integer">
            <column name="collateralid"/>
            <generator class="identity"/>
        </id>
        <property name="referenceno" type="string">
            <column name="referenceno" length="50"/>
        </property>
        <property name="collateralstatus" type="string">
            <column name="collateralstatus" length="50"/>
        </property>
        <property name="appraisalstatus" type="string">
            <column name="appraisalstatus" length="50"/>
        </property>
        <property name="dateencoded" type="timestamp">
            <column name="dateencoded" length="23"/>
        </property>
        <property name="encodedby" type="string">
            <column name="encodedby" length="50"/>
        </property>
        <property name="securitytype" type="string">
            <column name="securitytype" length="50"/>
        </property>
        <property name="securitycode" type="string">
            <column name="securitycode" length="50"/>
        </property>
        <property name="securitydesc" type="string">
            <column name="securitydesc" length="100"/>
        </property>
        <property name="isinno" type="string">
            <column name="isinno" length="50"/>
        </property>
        <property name="issuername" type="string">
            <column name="issuername" length="100"/>
        </property>
        <property name="unitvalue" type="big_decimal">
            <column name="unitvalue" precision="20"/>
        </property>
        <property name="rating" type="string">
            <column name="rating" length="50"/>
        </property>
        <property name="maturitydate" type="timestamp">
            <column name="maturitydate" length="23"/>
        </property>
        <property name="fundname" type="string">
            <column name="fundname" length="100"/>
        </property>
        <property name="schemename" type="string">
            <column name="schemename" length="100"/>
        </property>
        <property name="intpayout" type="big_decimal">
            <column name="intpayout" precision="20"/>
        </property>
        <property name="bondduration" type="string">
            <column name="bondduration" length="50"/>
        </property>
        <property name="securityprice" type="big_decimal">
            <column name="securityprice" precision="20"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="300"/>
        </property>
    </class>
</hibernate-mapping>