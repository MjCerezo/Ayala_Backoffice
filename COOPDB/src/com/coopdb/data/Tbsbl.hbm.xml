<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbsbl" table="TBSBL" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="currentratesbl" type="big_decimal">
            <column name="currentratesbl" precision="16"/>
        </property>
        <property name="netunimpairedcapital" type="integer">
            <column name="netunimpairedcapital"/>
        </property>
    </class>
</hibernate-mapping>