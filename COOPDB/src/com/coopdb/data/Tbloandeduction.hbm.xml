<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbloandeduction" table="TBLOANDEDUCTION" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="appno" type="string">
            <column name="appno" length="50"/>
        </property>
        <property name="deductiontype" type="string">
            <column name="deductiontype" length="50"/>
        </property>
        <property name="credittoaccountnumber" type="string">
            <column name="credittoaccountnumber" length="50"/>
        </property>
        <property name="deductionamount" type="big_decimal">
            <column name="deductionamount" precision="18"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="iscbu" type="boolean">
            <column name="iscbu"/>
        </property>
    </class>
</hibernate-mapping>