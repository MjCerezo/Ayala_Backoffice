        <property name="startdate" type="timestamp">
            <column name="startdate" length="23"/>
        </property>
        <property name="enddate" type="timestamp">
            <column name="enddate" length="23"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="100"/>
