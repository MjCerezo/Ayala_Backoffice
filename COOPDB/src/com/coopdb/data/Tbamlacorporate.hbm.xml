        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="amlaliststatus" type="string">
            <column name="amlaliststatus" length="50"/>
        </property>
        <property name="amlalistsource" type="string">
            <column name="amlalistsource" length="50"/>
