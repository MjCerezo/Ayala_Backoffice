<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbtransaction" table="TBTRANSACTION" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="txcode" type="string">
            <column name="txcode" length="10"/>
        </property>
        <property name="txname" type="string">
            <column name="txname" length="100"/>
        </property>
    </class>
</hibernate-mapping>