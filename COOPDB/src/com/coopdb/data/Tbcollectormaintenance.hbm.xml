<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbcollectormaintenance" table="TBCOLLECTORMAINTENANCE" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="collectorid" type="string">
            <column name="collectorid" length="50"/>
        </property>
        <property name="collectouserrname" type="string">
            <column name="collectouserrname" length="50"/>
        </property>
        <property name="collectorname" type="string">
            <column name="collectorname" length="300"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="branchcode" type="string">
            <column name="branchcode" length="50"/>
        </property>
    </class>
</hibernate-mapping>