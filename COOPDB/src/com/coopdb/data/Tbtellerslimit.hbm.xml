<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbtellerslimit" table="TBTELLERSLIMIT" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="coopcode" type="string">
            <column name="coopcode" length="50"/>
        </property>
        <property name="branchcode" type="string">
            <column name="branchcode" length="50"/>
        </property>
        <property name="roleid" type="string">
            <column name="roleid" length="50"/>
        </property>
        <property name="limitamount" type="big_decimal">
            <column name="limitamount" precision="20"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
        <property name="lastupdated" type="timestamp">
            <column name="lastupdated" length="23"/>
        </property>
    </class>
</hibernate-mapping>