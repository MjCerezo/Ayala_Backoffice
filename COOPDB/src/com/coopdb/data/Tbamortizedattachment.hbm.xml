<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.coopdb.data.Tbamortizedattachment" table="TBAMORTIZEDATTACHMENT" catalog="AcaciaAyalaCoreDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="appno" type="string">
            <column name="appno" length="20"/>
        </property>
        <property name="attachmenttype" type="string">
            <column name="attachmenttype" length="50"/>
        </property>
        <property name="attachmentdetail" type="string">
            <column name="attachmentdetail" length="50"/>
        </property>
        <property name="amount" type="big_decimal">
            <column name="amount" precision="20"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="username" type="string">
            <column name="username" length="50"/>
        </property>
    </class>
</hibernate-mapping>