<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.loansdb.data.Tbamlalistmain" table="TBAMLALISTMAIN" catalog="CIFSDBUAT" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="amlalistid" type="string">
            <column name="amlalistid" length="50"/>
            <generator class="assigned"/>
        </id>
        <property name="cifno" type="string">
            <column name="cifno" length="50"/>
        </property>
        <property name="fullname" type="string">
            <column name="fullname" length="250"/>
        </property>
        <property name="dateofbirth" type="timestamp">
            <column name="dateofbirth" length="23"/>
        </property>
        <property name="dateofincorporation" type="timestamp">
            <column name="dateofincorporation" length="23"/>
        </property>
        <property name="startdate" type="timestamp">
            <column name="startdate" length="23"/>
        </property>
        <property name="enddate" type="timestamp">
            <column name="enddate" length="23"/>
        </property>
        <property name="status" type="string">
            <column name="status" length="20"/>
        </property>
        <property name="amlalistsource" type="string">
            <column name="amlalistsource" length="50"/>
        </property>
        <property name="customertype" type="string">
            <column name="customertype" length="20"/>
        </property>
        <property name="createddate" type="timestamp">
            <column name="createddate" length="23"/>
        </property>
        <property name="isopenended" type="boolean">
            <column name="isopenended"/>
        </property>
    </class>
</hibernate-mapping>