<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.loansdb.data.Tbloanapprovalmatrix" table="TBLOANAPPROVALMATRIX" catalog="LOANSDBSIT" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="transactiontype" type="string">
            <column name="transactiontype" length="50"/>
        </property>
        <property name="loanproduct" type="string">
            <column name="loanproduct" length="50"/>
        </property>
        <property name="level1approver" type="string">
            <column name="level1approver" length="50"/>
        </property>
        <property name="level2approver" type="string">
            <column name="level2approver" length="50"/>
        </property>
        <property name="level3approver" type="string">
            <column name="level3approver" length="50"/>
        </property>
        <property name="level1requiredapproval" type="integer">
            <column name="level1requiredapproval"/>
        </property>
        <property name="level2requiredapproval" type="integer">
            <column name="level2requiredapproval"/>
        </property>
        <property name="level3requiredapproval" type="integer">
            <column name="level3requiredapproval"/>
        </property>
        <property name="level1rule" type="string">
            <column name="level1rule" length="50"/>
        </property>
        <property name="level2rule" type="string">
            <column name="level2rule" length="50"/>
        </property>
        <property name="level3rule" type="string">
            <column name="level3rule" length="50"/>
        </property>
        <property name="level1limit" type="big_decimal">
            <column name="level1limit" precision="20"/>
        </property>
        <property name="level2limit" type="big_decimal">
            <column name="level2limit" precision="20"/>
        </property>
        <property name="level3limit" type="big_decimal">
            <column name="level3limit" precision="20"/>
        </property>
    </class>
</hibernate-mapping>