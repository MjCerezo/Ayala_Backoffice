<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.loansdb.data.Tbevalpolicyerror" table="TBEVALPOLICYERROR" catalog="LOANSPBBDB" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="evalreportid" type="integer">
            <column name="evalreportid"/>
        </property>
        <property name="appno" type="string">
            <column name="appno" length="50"/>
        </property>
        <property name="characteristics" type="string">
            <column name="characteristics" length="200"/>
        </property>
        <property name="value" type="string">
            <column name="value" length="200"/>
        </property>
        <property name="remarks" type="string">
            <column name="remarks" length="1000"/>
        </property>
        <property name="approvallevel" type="string">
            <column name="approvallevel" length="200"/>
        </property>
    </class>
</hibernate-mapping>