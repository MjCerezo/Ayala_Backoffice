<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.loansdb.data.Tbcoa" table="TBCOA" catalog="LOANSDBSIT" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="accountno" type="string">
            <column name="accountno" length="50"/>
            <generator class="assigned"/>
        </id>
        <property name="acctdesc" type="string">
            <column name="acctdesc" length="100"/>
        </property>
        <property name="active" type="boolean">
            <column name="active"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
        <property name="dateupdated" type="timestamp">
            <column name="dateupdated" length="23"/>
        </property>
    </class>
</hibernate-mapping>