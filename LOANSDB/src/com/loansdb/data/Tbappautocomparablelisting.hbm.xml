<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.loansdb.data.Tbappautocomparablelisting" table="TBAPPAUTOCOMPARABLELISTING" catalog="LOANSDBSIT" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="appraisalreportid" type="string">
            <column name="appraisalreportid" length="20"/>
        </property>
        <property name="collateralid" type="integer">
            <column name="collateralid"/>
        </property>
        <property name="source" type="string">
            <column name="source" length="100"/>
        </property>
        <property name="price" type="big_decimal">
            <column name="price" precision="18"/>
        </property>
        <property name="lotarea" type="integer">
            <column name="lotarea"/>
        </property>
        <property name="description" type="string">
            <column name="description" length="500"/>
        </property>
    </class>
</hibernate-mapping>