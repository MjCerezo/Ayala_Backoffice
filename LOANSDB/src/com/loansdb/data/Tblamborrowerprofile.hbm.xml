<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.loansdb.data.Tblamborrowerprofile" table="TBLAMBORROWERPROFILE" catalog="LOANSDBSIT" schema="dbo" dynamic-insert="false" dynamic-update="false">
        <id name="id" type="integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="evalreportid" type="integer">
            <column name="evalreportid"/>
        </property>
        <property name="appno" type="string">
            <column name="appno" length="20"/>
        </property>
        <property name="cifname" type="string">
            <column name="cifname" length="300"/>
        </property>
        <property name="businessaddress" type="string">
            <column name="businessaddress" length="300"/>
        </property>
        <property name="businessreg" type="string">
            <column name="businessreg" length="50"/>
        </property>
        <property name="incorporationdate" type="timestamp">
            <column name="incorporationdate" length="23"/>
        </property>
        <property name="industry" type="string">
            <column name="industry" length="10"/>
        </property>
        <property name="tin" type="string">
            <column name="tin" length="20"/>
        </property>
        <property name="clientsince" type="timestamp">
            <column name="clientsince" length="23"/>
        </property>
        <property name="originatingteam" type="string">
            <column name="originatingteam" length="20"/>
        </property>
        <property name="riskclassification" type="string">
            <column name="riskclassification" length="20"/>
        </property>
        <property name="prevriskclassification" type="string">
            <column name="prevriskclassification" length="20"/>
        </property>
        <property name="createdby" type="string">
            <column name="createdby" length="50"/>
        </property>
        <property name="datecreated" type="timestamp">
            <column name="datecreated" length="23"/>
        </property>
        <property name="updatedby" type="string">
            <column name="updatedby" length="50"/>
        </property>
        <property name="lastupdated" type="timestamp">
            <column name="lastupdated" length="23"/>
        </property>
        <property name="introtoborrower" type="string">
            <column name="introtoborrower"/>
        </property>
        <property name="financialanalysis" type="string">
            <column name="financialanalysis"/>
        </property>
    </class>
</hibernate-mapping>